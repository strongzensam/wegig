<%= form_for @gig_skill do |f|%>
  <div>
    <ul>
      <% if @gig_skill && @gig_skill.errors.any? %>
        <% @gig_skill.errors.full_messages.each do |message| %>
          <li class = "error"><%= message %></li>
        <% end %>
      <% end %>
  </ul>
</div>


<table>
  <tr><td><%= f.label :work_experience_id %></td>
  <td><%= f.select(:work_experience_id, @work_experiences.collect {|a| ["#{a.title}", a.id]}) %></td></tr>   
  <tr><td><%= f.label :skill_id %></td>
  <td><%= f.select(:skill_id, @skills.collect {|a| ["#{a.name}", a.id]}) %></td></tr> 
  </table>
  <%= f.submit %>
<% end %>