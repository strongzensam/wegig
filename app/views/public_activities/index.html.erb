<h1>Newsfeed</h1>
<div ng-app="app">
  <div ng-controller="publicActivityCtrl"ng-init="setup()">
  <div ng-repeat="publicActivity in publicActivities">
<!--   <p>{{publicActivity.trackable_type}}</p> -->
  </div>
<p><% @public_activities.each do |activity| %>
<% if activity.trackable_type == "WorkExperience" %>
<div id="newsfeed">
<h3><span class="text-blue text-uppercase"><%= activity.trackable_type.constantize.find(activity.trackable_id).title %></span> was added to the Database!</h3>
<br>
<p><%= activity.trackable_type.constantize.find(activity.trackable_id).description %></p>
<hr>
</div>
<%= render_activities(@public_activities, layout: :activity) %>
<% end %>
<% end %>
  </div>
</div>